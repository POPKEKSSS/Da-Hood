local Rainbow = Window:NewTab("golden and rainbow ropeds")
local RainbowSection = Rainbow:NewSection("Make rainbow and golden ropeds requires 6")

RainbowSection:NewButton("make rainbow ropeds", "ButtonInfo", function()
    local args = {
        [1] = "20036",
        [2] = 6,
        [3] = "Rainbow"
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
end)
    
    RainbowSection:NewButton("make golden ropeds ", "ButtonInfo", function()
        local args = {
            [1] = "10036",
            [2] = 6,
            [3] = "Golden"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
    end)

