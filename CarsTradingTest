local Library =
loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window =
Library.CreateLib("Cars Trading Tesy by Popkek V1", "Sentinel")
 
--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Dupe Car")

MainSection:NewButton("dupe", "Car", function()
    -- Script generated by Popkeksss --
    
    local args = {
        [1] = {
            ["10001"] = 0
        }
    }
    
    game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
    end)

    MainSection:NewButton("upgrade req 3", "ButtonInfo", function()
        -- Script generated by Popkeksss --
        
        local args = {
            [1] = {
                ["10011"] = 3
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)
