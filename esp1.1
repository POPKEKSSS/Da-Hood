-- Nesne verileri (örnek)
local objects = {
    {name = "Enemy1", x = 100, y = 200},
    {name = "Enemy2", x = 300, y = 150},
}

-- Ekranda gösterim fonksiyonu
function drawESP(object)
    -- Burada nesnenin ekrandaki konumunu hesaplayıp çizebilirsin
    print("ESP: " .. object.name .. " - Konum: (" .. object.x .. ", " .. object.y .. ")")
end

-- Ana döngü
while true do
    for _, obj in ipairs(objects) do
        drawESP(obj)
    end
    
    -- Döngüyü yavaşlatmak için bekleme süresi
    os.execute("sleep 1") -- 1 saniye bekle
end
