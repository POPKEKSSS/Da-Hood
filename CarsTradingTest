        -- Script generated by Popkeksss - credits to exx#9394
        
        local args = {
            [1] = {
                ["10042"] = 1
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)

        MoreSection:NewButton("Upgrade V3", "Car", function()
            -- Script generated by Popkeksss - credits to exx#9394
            
            local args = {
                [1] = {
                    ["10041"] = 1
                }
            }
            
            game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
            end)            
            
--Rainbow
local Rainbow = Window:NewTab("Rainbow V1")
local RainbowSection = Rainbow:NewSection("Make rainbow and golden Cars requires 6")

RainbowSection:NewButton("make rainbow Pets", "ButtonInfo", function()
    local args = {
        [1] = "20001",
        [2] = 6,
        [3] = "Rainbow"
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
end)
    
    RainbowSection:NewButton("make golden Cars", "ButtonInfo", function()
        local args = {
            [1] = "10001",
            [2] = 6,
            [3] = "Golden"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
    end)

--fuse
local fuse = Window:NewTab("Fuse V2")
local fuseSection = fuse:NewSection("upgrade rainbow Pets and golden")

fuseSection:NewButton("3 rb Cars", "ButtonInfo", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = {
            ["10001"] = 3
        }
    }
    
    game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
    end)

    fuseSection:NewButton("1 golden cars", "ButtonInfo", function()
        -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = {
                ["10042"] = 1
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end) 

 --Colored
local Colored = Window:NewTab("Colored items")
local ColoredSection = Colored:NewSection("Buy colored items!")

ColoredSection:NewButton("Buy colored Roller Skates", "Requires idk", function()
    local args = {
        [1] = "purchase",
        [2] = 30001,
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Coin"):WaitForChild("CoinMarketRE"):FireServer(unpack(args))
end)

ColoredSection:NewButton("Buy colored wings dragon", "Requires idk", function()
    local args = {
        [1] = "purchase",
        [2] = 40105,
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Coin"):WaitForChild("CoinMarketRE"):FireServer(unpack(args))
end)

ColoredSection:NewButton("Buy colored Roped", "Requires idk", function()
    local args = {
        [1] = "purchase",
        [2] = 300036,
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Coin"):WaitForChild("CoinMarketRE"):FireServer(unpack(args))
end)

