local Library = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Popkek", "Synapse") 
local Tab = Window:NewTab("Pets Trade Scrpit") local Section = Tab:NewSection("Pets Trade Scrpit")
local Colored = Window:NewTab("Colored items")
local ColoredSection = Colored:NewSection("Buy colored items!")

ColoredSection:NewButton("Buy colored Triwyern", "Requires idk", function()
    local args = {
        [1] = "purchase",
        [2] = 40096,
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Coin"):WaitForChild("CoinMarketRE"):FireServer(unpack(args))
end)

