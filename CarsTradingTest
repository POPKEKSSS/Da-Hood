local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Cars trading Free by Popkeksss Beta", "Sentinel")
 
--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Dupe Car")

MainSection:NewButton("dupe", "Car", function()
    -- Script generated by Popkeksss - credits to exx#9394
    
    local args = {
        [1] = {
            ["10001"] = 0
        }
    }
    
    game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
    end)

    MainSection:NewButton("upgrade req 3", "ButtonInfo", function()
        -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = {
                ["10011"] = 3
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)

--More
local More = Window:NewTab("More V2")
local MoreSection = More:NewSection("Dupe the items u just fused")
MoreSection:NewButton("Upgrade V1", "Car", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = {
            ["10036"] = 1
