--Rainbow
local Rainbow = Window:NewTab("golden and rainbow ropeds")
local RainbowSection = Rainbow:NewSection("Make rainbow and golden ropeds requires 6")
 
RainbowSection:NewButton("make rainbow ropeds", "ButtonInfo", function()
    local args = {
        [1] = "20036",
        [2] = 6,
        [3] = "Rainbow"
    }
 
    game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
end)
 
    RainbowSection:NewButton("make golden ropeds ", "ButtonInfo", function()
        local args = {
            [1] = "10036",
            [2] = 6,
            [3] = "Golden"
        }
 
        game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
    end)
 
--fuse
local fuse = Window:NewTab("upgrade rainbow roped")
local fuseSection = fuse:NewSection("upgrade rainbow roped and golden")
 
fuseSection:NewButton("3 rb ropes", "ButtonInfo", function()
    -- Script generated by SimpleSpy - credits to exx#9394
 
    local args = {
        [1] = {
            ["30036"] = 3
        }
    }
 
    game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
    end)
 
    fuseSection:NewButton("1 golden ropeds", "ButtonInfo", function()
        -- Script generated by SimpleSpy - credits to exx#9394
 
        local args = {
            [1] = {
                ["20036"] = 1
            }
        }
 
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)
 
--Credits
local Credits = Window:NewTab("Credits")
local CreditsSection = Credits:NewSection("Subscribe to jngames for more!")
